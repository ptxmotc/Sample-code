@model List<SampleCode.Models.RailStation>
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h2>您的憑證：@ViewBag.ticket</h2>
            </hgroup>
            <p>
                @ViewBag.message
            </p>
        </div>
    </section>
}
<h3>取得台鐵資料內容:</h3>
<table class="table">
    <tr>
        <th>車站代碼</th>
        <th>車站中文名稱</th>
        <th>車站英文名稱</th>
        <th>地址</th>
        <th>電話</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.StationID</td>
            <td>@item.StationNameZh</td>
            <td>@item.StationNameEn</td>
            <td>@item.StationAddress</td>
            <td>@item.StationPhone</td>
        </tr>
    }
</table>
